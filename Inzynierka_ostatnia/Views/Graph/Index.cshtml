@model IEnumerable<Inzynierka_ostatnia.Models.InzynierEntities5>




@{
    ViewBag.Title = "Graph";



}
<head>
<link rel="stylesheet" href="~/StyleSheet1.css"/>    
</head>

<br/>
    <h2>Graf</h2>
    <div>
        @using (Html.BeginForm())
        {
            <p>
                Ród: (W11 lub W33)
                @Html.TextBox("Rod")
            </p>

            <div style="float:left;margin-right:100px">
                <p><strong>Osobnik żeński:</strong> <br /></p>
                <table>
                    <tbody>
                        <tr>
                            <td>Masa 8 tydzień:</td>
                            <td>@Html.TextBox("Masa8t_z")</td>  
                        </tr>
                        <tr>
                            <td>Masa 11 tydzień:</td>
                            <td>@Html.TextBox("Masa11t_z")</td>
                        </tr>
                        <tr>
                            <td>Przyrost masy (tylko W11):</td>
                            <td>@Html.TextBox("pmasy_z")</td>
                        </tr>
                        <tr>
                            <td>Procentowy przyrost masy (tylko W11):</td>
                            <td>@Html.TextBox("ppmasy_z")</td>
                        </tr>
                        <tr>
                            <td>Długość mostka (tylko W33):</td>
                            <td>@Html.TextBox("dlmos_z")</td>
                        </tr>
                        <tr>
                            <td>Długość przedramienia (tylko W33):</td>
                            <td>@Html.TextBox("dlpr_z")</td>
                        </tr>
                        <tr>
                            <td>Suma zniesionych jaj (tylko W11):</td>
                            <td>@Html.TextBox("sumaznjaj_z")</td>
                        </tr>
                        <tr>
                            <td>Masa jaj zważonych (tylko W11):</td>
                            <td>@Html.TextBox("masajaj_z")</td>
                        </tr>
                        <tr>
                            <td>Głębokość mięśni piersiowych (tylko W33):</td>
                            <td>@Html.TextBox("grmp_z")</td>
                        </tr>
                        <tr>
                            <td>Oszacowana masa mięśnia (tylko W33):</td>
                            <td>@Html.TextBox("omm_z")</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div style="float:inherit">
                <p><strong>Osobnik męski:</strong>   <br /></p>
                <table>
                    <tbody>
                        <tr>
                            <td>Masa 8 tydzień:</td>
                            <td>@Html.TextBox("Masa8t_m")</td>
                        </tr>
                        <tr>
                            <td>Masa 11 tydzień:</td>
                            <td>@Html.TextBox("Masa11t_m")</td>
                        </tr>
                        <tr>
                            <td>Przyrost masy (tylko W11):</td>
                            <td>@Html.TextBox("pmasy_m")</td>
                        </tr>
                        <tr>
                            <td>Procentowy przyrost masy (tylko W11):</td>
                            <td>@Html.TextBox("ppmasy_m")</td>
                        </tr>
                        <tr>
                            <td>Długość mostka (tylko W33):</td>
                            <td>@Html.TextBox("dlmos_m")</td>
                        </tr>
                        <tr>
                            <td>Długość przedramienia (tylko W33):</td>
                            <td>@Html.TextBox("dlpr_m")</td>
                        </tr>
                        <tr>
                            <td>Głębokość mięśni piersiowych (tylko W33):</td>
                            <td>@Html.TextBox("grmp_m")</td>
                        </tr>
                        <tr>
                            <td>Oszacowana masa mięśnia (tylko W33):</td>
                            <td>@Html.TextBox("omm_m")</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <br />
            <br />
            <br />
            <br />
            <div>
                <input type="submit" value="Szukaj" />
            </div>
            <br />
            <br />
            <br />
            <br />
        }

        
        

    <script type="text/javascript" src="~/Alchemy/scripts/vendor.js"></script>
    <script type="text/javascript" src="~/Alchemy/alchemy.js"></script>
        
        <div class="alchemy" id="alchemy"></div>
        <script type="text/javascript">

            var config = {
                dataSource: "http://localhost:21710/data/dane2.json",
                nodeTypes: { "role":["gasior","ges"]
                },
                nodeStyle:{
                    "all": {
                        "borderColor": "#127DC1",
                     },
                    "gasior":{
                        "color": "#C3C30E",
                    "selected": {
                    "color": "#ffffff",
                    },                    
                    "highlighted": {
                    "color": "#b4dcff"
                    }
                  }
                },

                forceLocked: true,
                directedEdges:true,
                graphHeight: function () { return 700},
                linkDistance: function () { return 200},
                caption: function (node) { return node.id; },
                nodeMouseOver: 'id',
            };

            alchemy = new Alchemy(config)
        </script>

        <a href="~/data/dane2.json">Dane 2</a>

        @*<script>
            alchemy.begin(config)
        </script>*@



    </div>


